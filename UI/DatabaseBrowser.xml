<Ui xmlns="http://www.blizzard.com/wow/ui/" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
    <Frame name="DT_DatabaseBrowser" 
        parent="UIParent" 
        inherits="UIPanelDialogTemplate"
        enableMouse="true" 
        movable="true" 
        frameStrata="DIALOG" 
        hidden="false" 
        toplevel="true"
        virtual="false">
        <Size>
            <AbsDimension x="800" y="538" />
        </Size>
        <Anchors>
            <Anchor point="CENTER" />
        </Anchors>
        <Frames>
            <EditBox name="DT_DatabaseBrowser_SearchBox" autoFocus="false" historyLines="1" inherits="SearchBoxTemplate" virtual="true">
                <Size x="460" y="26"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parent" x="20" y="-35" />
                </Anchors>
                <Scripts>
                    <OnEnterPressed function="DT_DatabaseBrowser_OnSearch" />
                </Scripts>
            </EditBox>
            <Button name="DT_DatabaseBrowser_SearchBtn" inherits="UIPanelButtonTemplate" text="Suchen" virtual="true">
                <Size x="96" y="24"/>
                <Anchors>
                    <Anchor point="RIGHT" relativeTo="DT_DatabaseBrowser_SearchBox" x="106" y="0"/>
                </Anchors>
                <Scripts>
                    <OnClick function="DT_DatabaseBrowser_OnSearch" />
                </Scripts>
            </Button>
        </Frames>
        <Scripts>
            <OnLoad function="DT_DatabaseBrowser_OnLoad" />
            <OnDragStart function="DT_DatabaseBrowser_OnDragStart" />
            <OnDragStop function="DT_DatabaseBrowser_OnDragStop" />
            <OnShow function="DT_DatabaseBrowser_OnShow" />
            <OnHide function="DT_DatabaseBrowser_OnHide" />
        </Scripts>
    </Frame>
</Ui>